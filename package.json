{
  "name": "dms-dto",
  "version": "1.0.2",
  "description": "",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "type": "commonjs",
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "build-rollup": "rollup -c",
    "build": "npm run build:es2015 && npm run build:esm && npm run build:cjs && npm run build:umd && npm run build:umd:min",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build:umd": "rollup dist/esm/index.js --format umd --name YourLibrary --sourceMap --output dist/umd/yourlibrary.js",
    "build:umd:min": "cd dist/umd && uglifyjs --compress --mangle --source-map --screw-ie8 --comments --o yourlibrary.min.js -- yourlibrary.js && gzip yourlibrary.min.js -c > yourlibrary.min.js.gz",
    "copy-to-dev": "cp -R dist ../dms-app/node_modules/dms-dto/ && cp -R dist ../dms-api/node_modules/dms-dto/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IrfanHasib/dms-dto.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IrfanHasib/dms-dto/issues"
  },
  "homepage": "https://github.com/IrfanHasib/dms-dto#readme",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "rollup": "^3.2.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.8.4"
  }
}
